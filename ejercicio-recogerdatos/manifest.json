{
  "manifest_version": 2,
  "name": "Notas",
  "version": "0.2",
  "minimum_chrome_version": "23",
  "icons": {
    "128": "img/icon.png" 
  },
  "app": {
    "background": {
      "scripts": ["js/main.js"]
    }
  },
  "permissions": [
   "background",
   {"fileSystem": ["write", "retainEntries", "directory"]},
    "storage"
  ],
 "file_handlers": {
    "text": {
      "types": [
          "text/*"
      ]
    }
  }
}
